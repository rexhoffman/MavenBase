<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns="http://maven.apache.org/DECORATION/1.3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">
  <publishDate format="dd MMM yyyy" position="left" />
  <version position="left" />
  <skin>
    <groupId>org.e-hoffman.maven</groupId>
    <artifactId>CloudySkySkin</artifactId>
    <version>1.0.0</version>
  </skin>
  <custom>
    <fluidoSkin>
      <breadcrumbDivider>»</breadcrumbDivider>
